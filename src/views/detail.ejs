<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Details</title>
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css">
</head>

<body style="background-image: url('/image/soccer.jpg');">
    <div class="detail-user">
        <a href="/"><button type="button" class="btn btn-primary">Back</button></a>
        <div>
            <label>ID:</label>
            <input name="id" value="<%= dataUser.id %>" />
        </div>
        <div>
            <label>First Name:</label>
            <input name="firstName" value="<%= dataUser.firstName %>" />
        </div>
        <div>
            <label>Last Name:</label>
            <input name="lastName" value="<%= dataUser.lastName %>" />
        </div>
        <div>
            <label>Email:</label>
            <input name="email" value="<%= dataUser.email %>" />
        </div>
        <div>
            <label>Address:</label>
            <input name="address" value="<%= dataUser.address %>" />
        </div>
    </div>
</body>

</html>